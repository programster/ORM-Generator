{
    "name": "programster/orm-generator",
    "type": "library",
    "description": "This package automates the creation of the table and object files for each of your database tables when you are using the irap/mysql-objects or programster/pgsql-objects package.",
    "keywords": ["ORM"],
    "homepage": "https://github.com/programster/package-orm-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "sdpagent@gmail.com",
            "homepage": "https://blog.programster.org/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0"
    },
    "autoload": {
        "psr-4": {
            "Programster\\OrmGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programster\\OrmGenerator\\Testing\\": "testing"
        }
    }
}
